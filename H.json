        "factoryId": "rss",
        "version": 1,
        "arguments": {
          "name": "nyaa-里",
          "description": "H~低调使用~",
          "iconUrl": "https://gitee.com/wan0ge/extract-pure-links/raw/Ani_Pages/favicon/nyaa.si_H.png",
          "searchConfig": {
            "searchUrl": "https://sukebei.nyaa.si/?page=rss&q={keyword}&c=0_0&f=0",
